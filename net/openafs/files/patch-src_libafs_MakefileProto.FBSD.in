+	  -e '/CRULE_OPT.*\<rand-fortuna\.c/s/CRULE_OPT/CRULE_OPT:N-mgeneral-regs-only/' \
+	  Makefile.common > $(KOBJ)/Makefile.common
 	-$(RM) -f h
 	-ln -fs ${KSRC}/sys h
 
@@ -116,10 +123,25 @@ include Makefile.common
 # We must live with its other pollution of targets and build rules.
