--- src/libafs/MakefileProto.FBSD.in.orig	2025-01-23 17:12:55 UTC
+++ src/libafs/MakefileProto.FBSD.in
@@ -59,6 +59,9 @@ CWARNFLAGS+= -Wno-redundant-decls
 WARNS= 1
