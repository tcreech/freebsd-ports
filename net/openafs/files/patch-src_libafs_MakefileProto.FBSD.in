 # setup for bsd.kmod.mk infrastructure
 .PATH:	${TOP_SRCDIR}/afs/FBSD
@@ -108,7 +111,11 @@ setup: $(KERNBUILDDIR)
 	-$(RM) $(KOBJ)/Makefile $(KOBJ)/Makefile.common
 	$(CP) Makefile $(KOBJ)/Makefile
 	echo ".include <bsd.kmod.mk>" >> $(KOBJ)/Makefile
